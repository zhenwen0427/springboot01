<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="springboot01.mapper.DeptMapper">
    <!-- 查询报警核实人列表 -->
    <select id="findAll" parameterType="Map" resultType="springboot01.entity.Dept">
        select
           deptno as deptno,
           dname as dname,
           db_source as db_source
        from dept
    </select>
</mapper>